{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": true,
        "paths": {
            "@helpers": [
                "./helpers"
            ],
            "@nnrUtil/*": [
                "./nnr/*"
            ],
            "@vercelUtil/*": [
                "./vercel/*"
            ],
            "@workflowUtil/*": [
                "./workflow/*"
            ],
            "@config/*": [
                "./config/*"
            ],
            "@store/*": [
                "./.github/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}
